<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Smoke and Regression TestSuit">
    <test name="Login Test">
        <classes>
            <class name="com.aqa.tests.LoginTest">
            <methods>
                <include name="userShouldLoginWithValidCredentials"/>
                <exclude name="userShouldNotBeLoggedInBeWithInvalidPassword"/>
            </methods>
            </class>

        </classes>
    </test>
</suite>